{
    "name": "oguzcandemircan/laravel-unique-sluggable",
    "description": "This package allows you to create unique slugs. It keeps all the slugs you define in the slugs table. It also satisfies all requests and directs it to the controller you define in your model.",
    "license": "MIT",
    "authors": [{
        "name": "OÄŸuzcan Demircan",
        "email": "oguzcandemircan@gmail.com",
        "homepage": "https://oguzcandemircan.com"
    }],
    "homepage": "https://github.com/oguzcandemircan/laravel-unique-sluggable",
    "keywords": [
        "Laravel",
        "Sluggable",
        "Slug",
        "Unique Slug"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "OguzcanDemircan\\LaravelUniqueSluggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OguzcanDemircan\\LaravelUniqueSluggable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OguzcanDemircan\\LaravelUniqueSluggable\\LaravelUniqueSluggableServiceProvider"
            ],
            "aliases": {
                "LaravelUniqueSluggable": "OguzcanDemircan\\LaravelUniqueSluggable\\Facades\\LaravelUniqueSluggable"
            }
        }
    }
}